      "auth": "netlify login",
      "verify": "netlify status",
      "docs": "https://docs.netlify.com/cli"
    },
    "firebase": {
      "name": "Firebase CLI",
      "install": "npm install -g firebase-tools",
      "auth": "firebase login",
      "verify": "firebase projects:list",
      "docs": "https://firebase.google.com/docs/cli"
    },
    "supabase": {
      "name": "Supabase CLI",
      "install": "brew install supabase/tap/supabase",
      "installWin": "scoop install supabase",
      "installNpm": "npm install -g supabase",

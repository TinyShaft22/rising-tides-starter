      "name": "Stripe CLI",
      "install": "brew install stripe/stripe-cli/stripe",
      "installWin": "scoop install stripe",
      "auth": "stripe login",
      "verify": "stripe config --list",
      "docs": "https://stripe.com/docs/cli"
    },
    "vercel": {
      "name": "Vercel CLI",
      "install": "npm install -g vercel",
      "auth": "vercel login",
      "verify": "vercel whoami",
      "docs": "https://vercel.com/docs/cli"
